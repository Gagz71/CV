                            <label for="g-recaptcha">reCAPTCHA</label>
                            <div class="g-recaptcha" data-sitekey="{{ google_recaptcha_site_key }}" id="g-recaptcha"></div>
                        </div>

                        {{ form_row(form.agreeTerms) }}
                        <div class="flex-row champs mt-3">
                            <span style="color: #0f9b0f">* </span> Champs obligatoires
                        </div>

                        {{ form_row(form.submit) }}
                        {# Fermeture du formulaire #}
                        {{ form_end(form) }}
